{
 "name": "NASCAR Vote Counter",
 "version": "2.0.1",
 "content_security_policy": "script-src 'self'; object-src 'self'",
 "manifest_version": 2,
 
 "description": "Reminds you to vote (preferably for Josh Wise) for the most popular NASCAR driver vote, while counting your votes.",

"icons": {
   "16":  "icon16.png",
   "48":  "icon48.png",
   "128": "icon128.png"
 },
"browser_action": {
	"default_icon": "icon19.png",
	"default_title": "Library manager",
	"default_popup": "popup.html"
  },
 "background": {
	"scripts": ["background.js"]
 },
 "permissions": [
     "tabs",
     "webRequest",
     "notifications",
     "*://connectapi.telescope.tv/*",
     "*://*.sprint.votenow.tv/*"
 ]
}